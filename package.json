{
  "name": "ookii",
  "version": "1.0.0",
  "description": "Ookii is a clone of splix for HummingBird",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "cross-env NODE_ENV=development npm run server",
    "clean": "rimraf dist",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.production.js",
    "build": "npm run clean && npm run build:webpack && cross-env NODE_ENV=production npm run server",
    "heroku": "webpack --config webpack.config.production.js && npm run server",
    "dev": "npm run start",
    "prod": "npm run build",
    "staging": "cross-env NODE_ENV=staging npm run server",
    "server": "nodemon index.js"
  },
  "author": "jcgertig <jcgertig@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.13.0",
    "color-it": "^1.2.4",
    "cross-env": "^2.0.1",
    "emojic": "^1.1.5",
    "express": "^4.14.0",
    "mongodb": "^2.2.9",
    "socket.io": "^1.4.8"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-1": "^6.13.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.4.0",
    "eslint-plugin-babel": "^3.3.0",
    "eventsource-polyfill": "^0.9.6",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "json-loader": "^0.5.4",
    "nodemon": "^1.10.2",
    "normalize.css": "^4.2.0",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.11.1",
    "precss": "^1.4.0",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2"
  }
}
